<%if(success && success.length){%>
<div class="alert alert-success alert-dismissible fade show col-md-6 mt-1 offset-md-3">
    <%= success %>
    <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
    ></button>
</div>
<%}%>


<%if(error && error.length){%>
<div class="alert alert-danger alert-dismissible fade show col-md-6 mt-1 offset-md-3">
    <%= error %>
    <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
    ></button>
</div>
<%}%>